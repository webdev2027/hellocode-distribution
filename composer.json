{
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        },
        {
            "type": "git",
            "url": "https://github.com/webdev2027/hellocode-distribution.git"
        }
    ],
    "name": "ssa/hellocode",
    "type": "typo3-cms-extension",
    "description": "FLUID Templates & TypoScript",
    "version": "1.0.0",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "authors": [
        {
            "name": "Stefan Sajok",
            "role": "Developer",
            "homepage": "https://www.hellocode.de/"
        }
    ],
    "license": [
        "GPL-3.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ssa\\Hellocode\\": "Classes"
        }
    },
    "replace": {
        "hellocode": "self.version",
        "typo3-ter/hellocode": "self.version"
    }
}