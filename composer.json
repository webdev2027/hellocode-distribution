{
    "name": "stsa/hellocode",
    "type": "typo3-cms-extension",
    "description": "FLUID Templates & TypoScript",
    "homepage": "https://www.hellocode.de/",
    "keywords": [
        "TYPO3",
        "CMS",
        "extension"
    ],
    "authors": [
        {
            "name": "Stefan Sajok",
            "email": "hello@hellocode.de",
            "role": "Developer",
            "homepage": "https://www.hellocode.de/"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Stsa\\Hellocode\\": "Classes/"
        }
    },
    "replace": {
        "hellocode": "self.version",
        "typo3-ter/hellocode": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "stsa_hellocode"
        }
    }
}