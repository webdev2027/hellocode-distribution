{
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/webdev2027/hellocode-distribution.git"
        }
    ],
    "name": "stsa/hellocode",
    "type": "typo3-cms-extension",
    "description": "FLUID Templates & TypoScript",
    "homepage": "https://www.hellocode.de/",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "authors": [
        {
            "name": "Stefan Sajok",
            "email": "hello@hellocode.de",
            "role": "Developer",
            "homepage": "https://www.hellocode.de/"
        }
    ],
    "license": [
        "GPL-3.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^8.7.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Stsa\\Hellocode\\": "Classes"
        }
    },
    "replace": {
        "hellocode": "self.version",
        "typo3-ter/hellocode": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    }
}